    "solc-0.8": "npm:solc@0.8.7",
    "solhint": "3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "0.7.17",
    "solidity-docgen": "0.5.14",
    "sort-package-json": "1.51.0",
    "standard-version": "9.3.1",
